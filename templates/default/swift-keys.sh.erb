# Generated by Chef
 
# Credentials etc for accessing Swift
export SWIFT_USERNAME="<%= @os_username %>"
export SWIFT_PASSWORD="<%= @os_password %>"
export SWIFT_AUTHURL="<%= @os_auth_url %>"
export SWIFT_TENANTNAME="<%= @os_tenant_name %>"
export SWIFT_AUTHVERSION="<%= @os_auth_version %>"

# Used to encrypt backup contents before uploading.
# Also required to decrypt contents, unless using public key encryption.
export PASSPHRASE="<%= @passphrase %>"
